{
  "name": "oauth proxy",
  "description": "Simple proxy for the oauth service to keep the client_secret and client_id secret",
  "repository": "https://github.com/gotik/oauth-proxy",
  "keywords": ["node", "oauth", "proxy"],
  "env": {
    "OAUTH_CLIENT_ID": {
      "description": "Your oauth client_id"
    },
    "OAUTH_CLIENT_SECRET": {
      "description": "Your oauth client_secret"
    },
    "API_URL": {
      "description": "Your authentication endpoint"
    }
  }
}
